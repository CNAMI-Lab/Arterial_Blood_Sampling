#Unit test for flowcorrectionfxn:
import unittest
from flowcorrectionRO948 import flowcorrectionfxn


class MyTestfirstfxn (unittest.TestCase):
    def test_flowcorrectionfxn(self):
        # Expected output
        expected_correction = 0.958508395
        halflife = 109.771
        RTmetab1 = 1.04
        RTmetab2 = 7.19
        RTtracer = 9.01

        areaMetab1 = 1.234
        areaMetab2 = .613
        areaTracer = 50.143
        # Call the function
        actual_correction = flowcorrectionfxn(halflife, RTmetab1, RTmetab2, RTtracer, areaMetab1, areaMetab2, areaTracer)

        # Compare the actual and expected results
        self.assertEqual(actual_correction, expected_correction)

if __name__ == '__main__':
    unittest.main()

#The error: AssertionError: 0.9619865754653268 != 0.958508395 

#The unit test for columnA:
import unittest
from flowcorrectionRO948 import columnA

class MyTestCase(unittest.TestCase):
    def test_columnA(self):
        # plasma test

        flow_metab1 = 2.484260846
        flow_metab2 = 0.962208517
        flow_tracer = 79.61778749
        lostASEM = .030258  # E2

        # Expected output
        expected_correctedintact = 0.987138062

        # Call the function
        actual_correctedintact = columnA(flow_metab1, flow_metab2, flow_tracer, lostASEM)

        # Compare the actual and expected results
        self.assertEqual(actual_correctedintact, expected_correctedintact)


if __name__ == '__main__':
    unittest.main()

#the error: AssertionError: 0.9875109416681658 != 0.987138062
